{
  "name": "drupal/rocketship_core",
  "description": "Contains core functionality for Dropsolid websites.",
  "type": "drupal-module",
  "homepage": "https://drupal.org/project/rocketship_core",
  "authors": [
    {
      "name": "Nick Vanpraet",
      "role": "Maintainer"
    }
  ],
  "support": {
    "docs": "https://drupal.org/project/rocketship_core"
  },
  "require": {
    "drupal/config_import_locale": "~1.0",
    "drupal/admin_toolbar": "~1.17",
    "drupal/config_update": "~1.2",
    "drupal/ctools": "~3.0",
    "drupal/ds": "~3.3",
    "drupal/easy_breadcrumb": "~1.5",
    "drupal/field_group": "3.0.0-beta1",
    "drupal/inline_entity_form": "1.0-rc1",
    "drupal/mailsystem": "~4.1",
    "drupal/menu_trail_by_path": "~1.0",
    "drupal/page_manager": "4.0.0-beta3",
    "drupal/panels": "~4.4",
    "drupal/search_api": "~1.11",
    "drupal/swiftmailer": "1.0-beta2",
    "drupal/token": "~1.0",
    "drupal/video_embed_field": "~1.3",
    "drupal/plugin": "~2.5",
    "drupal/linkit": "~5.0",
    "drupal/editor_advanced_link": "~1.4",
    "drupal/editor_file": "~1.2",
    "drupal/advagg": "~3.3",
    "drupal/webform": "^5.1",
    "drupal/paragraphs": "~1.5",
    "drupal/smart_trim": "~1.0",
    "drupal/config_split": "~1.2",
    "drupal/config_ignore": "~2.1",
    "drupal/scheduler": "~1.0",
    "drupal/taxonomy_access_fix": "~2.6",
    "drupal/google_tag": "~1.1",
    "drupal/metatag": "~1.10",
    "drupal/schema_metatag": "~1.3",
    "drupal/pathauto": "~1.0",
    "drupal/simple_sitemap": "~3.1",
    "drupal/migrate_source_csv": "~2.2",
    "drupal/colorbox": "~1.4",
    "drupal/slick": "~1.0",
    "drupal/blazy": "1.0-rc4",
    "vardot/blazy": "~1.8",
    "drupal/paragraphs_asymmetric_translation_widgets": "1.0-beta1",
    "drupal/migrate_plus": "^4.1",
    "drupal/migrate_tools": "^4.1",
    "drupal/maxlength": "1.0-beta2",
    "drupal/redirect": "~1.0",
    "drupal/components": "~1.0",
    "drupal/focal_point": "1.0-beta6",
    "drupal/userprotect": "~1.0",
    "drupal/roleassign": "1.0-alpha2",
    "drupal/view_unpublished": "1.0-alpha1",
    "drupal/block_content_permissions": "~1.6",
    "drupal/image_effects": "~2.1",
    "drupal/file_mdm": "~1.1",
    "drupal/node_title_help_text": "~1.0",
    "drupal/config_sync": "2.0-beta4",
    "drupal/menu_admin_per_menu": "^1.0",
    "drupal/config_distro": "1.0-alpha3",
    "drupal/config_provider": "2.0-alpha6",
    "drupal/config_filter": "~1.2",
    "drupal/config_merge": "1.0-alpha2",
    "drupal/node_keep": "~1.0",
    "drupal/config_rewrite": "~1.1",
    "drupal/better_exposed_filters": "3.0-alpha5",
    "drupal/rocketship_paragraphs": "^1.0"
  },
  "minimum-stability": "dev",
  "extra": {
    "patches": {
      "drupal/easy_breadcrumb": {
          "Breadcrumb markup should allow some tags such as italics or strong": "https://www.drupal.org/files/issues/2019-09-18/easy_breadcrumb.2990464-36.patch"
      },
      "drupal/paragraphs_asymmetric_translation_widgets": {
        "Add support for experimental widget -- https://www.drupal.org/project/paragraphs_asymmetric_translation_widgets/issues/2959900": "https://www.drupal.org/files/issues/2018-11-12/2959900-13.patch"
      },
      "drupal/core": {
        "Incorrect translation status for duplicated entities. -- https://www.drupal.org/project/drupal/issues/3009016#comment-12828375": "https://www.drupal.org/files/issues/2018-10-25/3009016-6.patch"
      },
      "drupal/linkit": {
        "Add support for multilanguage - https://www.drupal.org/project/linkit/issues/2886455": "https://www.drupal.org/files/issues/2019-05-03/linkit-multilanguage_matching-2886455-19.patch"
      },
      "drupal/page_manager": {
        "Page Manager currently not working with CTools 3.1+": "https://www.drupal.org/files/issues/2019-02-26/3033057-ctools31-15.patch",
        "Variants take entity label not the variant title https://www.drupal.org/node/2665328": "https://www.drupal.org/files/issues/2018-07-04/2665328-variants-take-entity-label-83.patch"
      },
      "drupal/config_ignore": {
        "Ignore configuration storage collections?": "https://www.drupal.org/files/issues/2018-09-27/2973431.8.patch"
      },
      "drupal/field_group": {
        "This patch will never get into the module because they went into a different direction - https://www.drupal.org/project/field_group/issues/2822299": "https://www.drupal.org/files/issues/2019-04-25/field_group-double_escaped_titles-2822299-24.patch"
      },
      "drupal/node_title_help_text": {
        "Fix missing variable": "https://www.drupal.org/files/issues/2019-07-02/node_title_help_text-undefined_variable_description-3065429-2.patch",
        "Only nodes": "https://www.drupal.org/files/issues/2019-07-25/ief-only-nodes.patch"
      },
      "drupal/search_api": {
        "Rendered HTML Output doesnt respect activeLanguage completely": "https://www.drupal.org/files/issues/2019-05-28/3035977-20--rendered_item_language.patch"
      }
    }
  }
}
